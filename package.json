    "devDependencies": {
        "@eslint/js": "^9.34.0",
        "@types/jest": "^29.2.4",
        "@types/n3": "^1.26.0",
        "@typescript-eslint/eslint-plugin": "^8.42.0",
        "@typescript-eslint/parser": "^8.42.0",
        "jest": "^29.3.1",
        "supertest": "^6.3.3",
